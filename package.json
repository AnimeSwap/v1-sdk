  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "esbuild": "^0.15.18",
    "eslint": "^8.29.0",
    "jest": "29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "aptos": "^1.4.0",
    "axios": "^1.2.0",
    "decimal.js": "^10.4.3"
